{
  "name": "o7",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "yarn run buildtwo && node -r tsconfig-paths/register dist/index.js",
    "dev": "nodemon",
    "dev:debug": "nodemon --inspect",
    "buildtwo": "rimraf dist/ && tsc",
    "build": "rimraf dist/ && tsc && copyfiles -u 1 src/**/*.json dist",
    "forever": "forever start dist/index.js",
    "stop": "forever stop dist/index.js"
  },
  "watch": {
    "build": {
      "patterns": [
        "src"
      ],
      "extensions": "ts,tsx,json",
      "quiet": true,
      "legacyWatch": true,
      "delay": 2500,
      "runOnChangeOnly": false
    }
  },
  "author": "JB <jb@codecorsair.com>",
  "license": "MIT",
  "dependencies": {
    "@types/node-fetch": "^2.5.7",
    "discord-akairo": "^8.1.0",
    "discord.js": "^12.3.1",
    "forever": "^3.0.2",
    "forever-monitor": "^3.0.1",
    "fuse.js": "^6.4.1",
    "lodash": "^4.17.20",
    "moment": "^2.27.0",
    "mongodb": "^3.6.2",
    "node-fetch": "^2.6.1",
    "numeral": "^2.0.6",
    "svelte": "^3.25.0",
    "watch": "^1.0.2"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.161",
    "@types/mongodb": "^3.5.27",
    "@types/numeral": "0.0.28",
    "@types/ws": "^7.2.6",
    "copyfiles": "^2.3.0",
    "nodemon": "^2.0.4",
    "npm-watch": "^0.7.0",
    "rimraf": "^3.0.2",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.2"
  }
}
