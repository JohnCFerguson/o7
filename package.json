{
  "name": "o7",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "yarn run build && node dist/index.js",
    "dev": "nodemon",
    "dev:debug": "nodemon --inspect",
    "build": "rimraf dist/ && tsc && mkdirp dist/cache",
    "forever": "forever start dist/index.js",
    "stop": "forever stop dist/index.js",
    "winauth": "$env:GOOGLE_APPLICATION_CREDENTIALS=./gcloud-auth.json"
  },
  "watch": {
    "build": {
      "patterns": [
        "src"
      ],
      "extensions": "ts,tsx,json",
      "quiet": true,
      "legacyWatch": true,
      "delay": 2500,
      "runOnChangeOnly": false
    }
  },
  "author": "JB <jb@codecorsair.com>",
  "license": "MIT",
  "dependencies": {
    "@google-cloud/storage": "^5.12.0",
    "@google-cloud/vision": "^2.3.7",
    "@types/murmurhash": "^2.0.0",
    "@types/node-fetch": "^2.5.12",
    "arangojs": "^7.5.0",
    "discord-akairo": "^8.1.0",
    "discord.js": "^12.5.3",
    "forever": "^4.0.1",
    "forever-monitor": "^3.0.3",
    "fuse.js": "^6.4.6",
    "image-size": "^1.0.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "mongodb": "^4.1.0",
    "murmurhash": "^2.0.0",
    "neo4j-driver": "^4.3.2",
    "node-fetch": "^2.6.1",
    "numeral": "^2.0.6",
    "request": "^2.88.2",
    "shortid": "^2.2.16",
    "svelte": "^3.42.1",
    "tesseract.js": "^2.1.5",
    "watch": "^1.0.2"
  },
  "devDependencies": {
    "@types/image-size": "^0.8.0",
    "@types/lodash": "^4.14.172",
    "@types/mongodb": "^4.0.7",
    "@types/numeral": "^2.0.1",
    "@types/request": "^2.48.7",
    "@types/shortid": "^0.0.29",
    "@types/ws": "^7.4.7",
    "copyfiles": "^2.4.1",
    "mkdirp": "^1.0.4",
    "nodemon": "^2.0.12",
    "npm-watch": "^0.11.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5"
  }
}
